{
  "name": "n8n-ai-assistant",
  "version": "0.1.0",
  "description": "AI-powered assistant for N8N workflows with visual enhancements",
  "keywords": [
    "n8n-community-node-package",
    "n8n-node-dev",
    "ai",
    "assistant",
    "workflow"
  ],
  "license": "MIT",
  "homepage": "https://n8n.io",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/n8n-ai-assistant.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js src"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/OpenAiApi.js",
      "dist/credentials/OpenRouterApi.js"
    ],
    "nodes": [
      "dist/nodes/N8nAiAssistant.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.22",
    "@types/node": "^18.19.110",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-plugin-n8n-nodes-base": "^1.14.0",
    "gulp": "^4.0.2",
    "n8n-core": "^0.145.0",
    "n8n-workflow": "^0.145.0",
    "prettier": "^2.8.8",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "n8n-nodes-base": "^0.225.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
